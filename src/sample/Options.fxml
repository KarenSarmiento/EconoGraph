<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<GridPane alignment="center" hgap="10" prefHeight="800.0" prefWidth="1200.0" stylesheets="/sample/sample.css" vgap="10" xmlns="http://javafx.com/javafx/8.0.76-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
    <children>
        <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="21.0" prefWidth="1200.0" text="Create your own new Graph" textAlignment="CENTER" GridPane.columnIndex="0" GridPane.columnSpan="2" GridPane.rowIndex="0" />
        <Label alignment="CENTER" prefHeight="21.0" prefWidth="600.0" text="Title: " GridPane.columnIndex="0" GridPane.rowIndex="1" />
        <TextField prefHeight="31.0" prefWidth="292.0" text="Title" GridPane.columnIndex="1" GridPane.rowIndex="1" />
        <Label text="Select a canvas:" GridPane.columnIndex="0" GridPane.columnSpan="2" GridPane.rowIndex="2" />
        <ScrollPane prefHeight="507.0" prefWidth="764.0" GridPane.columnIndex="0" GridPane.columnSpan="2" GridPane.rowIndex="2">
            <content>
                <FlowPane prefHeight="630.0" prefWidth="1150.0">
                    <Button fx:id="macroB" mnemonicParsing="false" onAction="#test" text="Microeconomic Equilibrium" />
                    <Button fx:id="microB" mnemonicParsing="false" onAction="#test" text="Macroeconomic Equilibrium" />
                    <Button fx:id="subsidyB" mnemonicParsing="false" onAction="#test" text="Labour Force" />
                    <Button fx:id="taxB" mnemonicParsing="false" onAction="#test" text="Subsidy" />
                </FlowPane>
            </content>
        </ScrollPane>
        <Button fx:id="doneB" alignment="CENTER_RIGHT" mnemonicParsing="false" onAction="#newGraphToGraphMaker" text="Done!" GridPane.columnIndex="0" GridPane.rowIndex="3" />
        <Button fx:id="cancelB" mnemonicParsing="false" onAction="#setScreenToLibrary" text="Cancel" GridPane.columnIndex="1" GridPane.rowIndex="3" />
    </children>
   <columnConstraints>
      <ColumnConstraints maxWidth="570.0" minWidth="500.0" prefWidth="560.0" />
      <ColumnConstraints maxWidth="570.0" minWidth="500.0" prefWidth="560.0" />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints maxHeight="100.0" minHeight="32.0" prefHeight="32.0" />
      <RowConstraints maxHeight="99.0" minHeight="15.0" prefHeight="31.0" />
      <RowConstraints maxHeight="700.0" minHeight="600.0" prefHeight="650.0" />
      <RowConstraints maxHeight="40.0" minHeight="0.0" prefHeight="40.0" />
   </rowConstraints>


</GridPane>
